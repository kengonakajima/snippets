# PostgreSQL接続設定
DATABASE_URL=postgres://postgres:postgres@localhost:5432/pg_bench

# ベンチマーク設定
BENCH_TOTAL_ROWS=10000
BENCH_USER_COUNT=1000
BENCH_BATCH_SIZE=500
SINGLE_SELECT_REQUESTS=1000
RANGE_SELECT_REQUESTS=300

# 暗号化鍵設定
COMMON_KEY=replace-with-common-key
USER_BASE_KEY=replace-with-user-base-key
PBKDF2_ITERATIONS=100000
PBKDF2_KEY_LENGTH=32
PBKDF2_DIGEST=sha256

# 結果出力先
RESULTS_DIR=results
